<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myschool.game">

    <application
        android:name="com.myschool.game.Init"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"> <!-- Instanciation d'un objet Init Ã©tendant Application -->
        <activity
            android:name="com.myschool.game.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.myschool.game.GameActivity"
            android:label="@string/title_activity_game"></activity>
    </application>

</manifest>
